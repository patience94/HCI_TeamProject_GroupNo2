[loggers]
keys=root

[logger_root]
level=DEBUG
handlers=logfile

[formatters]
keys=logfileformatter

[handlers]
keys=logfile

[formatter_logfileformatter]
format=%(asctime)s %(name)-12s: %(levelname)s %(message)s

[handler_logfile]
class=handlers.RotatingFileHandler
level=DEBUG
args=('c:\\testing.log',2000,100)
formatter=logfileformatter
